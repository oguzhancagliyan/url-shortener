ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://0.0.0.0:8080

# Runtime-selectable provider: Couchbase | SqlServer | MySQL | PostgreSQL | MongoDB
DbProvider=PostgreSQL

# SQL Server
ConnectionStrings__SqlServer=Server=localhost,1433;Database=urlshortener;User Id=sa;Password=YourStrong(!)Password;TrustServerCertificate=True

# MySQL
ConnectionStrings__MySql=Server=localhost;Port=3306;Database=urlshortener;User Id=root;Password=root

# PostgreSQL
ConnectionStrings__PostgreSql=Host=localhost;Port=5432;Database=urlshortener;Username=postgres;Password=postgres

# MongoDB
ConnectionStrings__MongoDb=mongodb://localhost:27017
Database__DatabaseName=urlshortener

# Couchbase
ConnectionStrings__Couchbase=couchbase://localhost
Database__BucketName=urlshortener
Database__ScopeName=_default
Database__CollectionName=shorturls

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
